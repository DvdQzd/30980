<section class="mt-5">
    <h1 class="text-danger"> Historial</h1>
        <% if ( personas.length === 0) { %> 
            <h2 class="bg-danger rounded text-center m-4">No se encontraron datos</h2>
        <% } else {%> 
    <table class="table table-dark">
        <tr class="text-warning">
            <th>Nombre</th>
            <th>descripcion</th>
            <th>c√≥digo</th>
            <th>foto</th>
            <th>precio</th>
            <th>stock</th>
        </tr>
        <% personas.forEach(function(user) { %> 
        <tr>
            <td><%= user.nombre %></td>
            <td><%= user.descripcion %></td>
            <td><%= user.codigo %></td>
            <td><%= user.foto %></td>
            <td><%= user.precio %></td>
            <td><%= user.stock %></td>
        </tr>
        <% }) %> 
    </table>
    <% } %> 
</section>